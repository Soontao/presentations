{
  "name": "presentations",
  "version": "1.6.0",
  "description": "Presentations",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "packageManager": "pnpm@7.16.0",
  "scripts": {
    "server": "marp -p --server ./src",
    "release": "npx standard-version --commit-all --no-verify",
    "build": "node scripts/build.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Soontao/presentations.git"
  },
  "author": "Theo Sun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Soontao/presentations/issues"
  },
  "marp": {
    "themeSet": "./theme",
    "engine": "./engine.js"
  },
  "homepage": "https://github.com/Soontao/presentations#readme",
  "dependencies": {
    "@marp-team/marp-cli": "2.2.0",
    "@marp-team/marp-core": "3.4.0",
    "@marp-team/marpit": "2.4.2",
    "@newdash/newdash": "5.21.4",
    "@openui5/ts-types": "1.108.1",
    "@textlint/markdown-to-ast": "12.2.3",
    "@types/debug": "4.1.7",
    "@types/markdown-it": "12.2.3",
    "colors": "1.4.0",
    "debug": "4.3.4",
    "fast-glob": "3.2.12",
    "git-rev-sync": "3.0.2",
    "mkdirp": "1.0.4",
    "pretty": "2.0.0",
    "replace": "1.2.2",
    "textlint": "12.2.3"
  }
}