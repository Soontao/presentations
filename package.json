{
  "name": "presentations",
  "version": "1.6.0",
  "description": "Presentations",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "packageManager": "pnpm@8.5.1",
  "scripts": {
    "server": "marp -p --server ./src",
    "release": "npx standard-version --commit-all --no-verify",
    "build": "node scripts/build.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Soontao/presentations.git"
  },
  "author": "Theo Sun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Soontao/presentations/issues"
  },
  "marp": {
    "themeSet": "./theme",
    "engine": "./engine.js"
  },
  "homepage": "https://github.com/Soontao/presentations#readme",
  "dependencies": {
    "@marp-team/marp-cli": "3.2.1",
    "@marp-team/marp-core": "3.8.1",
    "@marp-team/marpit": "2.5.1",
    "@newdash/newdash": "5.22.0",
    "@openui5/ts-types": "1.118.0",
    "@textlint/markdown-to-ast": "13.3.3",
    "@types/debug": "4.1.8",
    "@types/markdown-it": "13.0.1",
    "colors": "1.4.0",
    "debug": "4.3.4",
    "fast-glob": "3.3.1",
    "git-rev-sync": "3.0.2",
    "markdown-it-html5-media": "^0.7.1",
    "mkdirp": "2.1.6",
    "pretty": "2.0.0",
    "replace": "1.2.2",
    "textlint": "13.3.3"
  }
}